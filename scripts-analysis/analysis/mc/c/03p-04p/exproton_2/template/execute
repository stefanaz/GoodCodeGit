#!/bin/sh
#
# execute analyses
#
# parameters
#
# $1: path and name of merged histogram-file
#
if [ -z $1 ]; then
    echo ""
    echo "Please give merged histogram-file"
    echo ""
    echo "Usage: execute [outputname]"
    echo ""
    exit
fi
#
nice -20 analysis.exe mc_c_exproton_2.cards
merger.exe MERGE histos.root $1 mc/03p-04p/c/exproton_2

